{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "date": {
            "description": "Date of the gazzete",
            "type": "string",
            "format": "date"
        },
        "file_url": {
            "type": "string",
            "examples": [
                "https://diariooficial.jaboatao.pe.gov.br/10-de-outubro-de-2020-xxx-no-199-jaboatao-dos-guararapes/?pdf=22609"
            ]
        },
        "is_extra_edition": {
            "description": "Determines if the edition of the gazzete is extra or not",
            "type": "boolean"
        },
        "territory_id": {
            "type": "string"
        },
        "power": {
            "type": "string",
            "enum": [
                "executive",
                "legislative",
                "executive_legislative"
            ]
        },
        "scraped_at":{
            "type": "string"
        },
        "files": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "properties": {
                    "url": {
                        "type": "string"
                    },
                    "path": {
                        "type": "string"
                    },
                    "checksum": {
                        "type": "string"
                    },
                    "status": {
                        "type": "string"
                    }
                }
            }
        }
    },
    "required": [
        "date",
        "file_url",
        "territory_id",
        "power",
        "scraped_at"
    ]
}